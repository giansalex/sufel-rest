{
    "alias": "composer.phar",
    "chmod": "0755",
    "directories": ["src"],
    "finder": [
        {
            "name": "*.php",
            "exclude": ["test" ,"tests", "Test", "Tests"],
            "in": "vendor"
        }
    ],
    "compression" : "GZ",
    "main" : "public/index.php",
    "output": "sufel.phar",
    "stub": true,
    "web": false,
    "banner": "Sufel Application",
    "shebang": false
}